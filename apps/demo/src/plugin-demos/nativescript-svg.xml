      navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="nativescript-svg" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <ScrollView class="h-full">
          <StackLayout>
            <svg:SVGImage src="~/images/spider.svg" width="335" height="269" />
            <svg:SVGImage src="{{svgFile}}" width="256" height="256" />
          </StackLayout>
        </ScrollView>
    </StackLayout>
</Page>
